    {
      "title": "Begin an Effort",
      "name": "Effort",
      "customClassname": "Effort",
      "fields": [
        {
          "name": "title",
          "label": "Title",
          "description": "Enter a clear and concise title that reflects the main focus of the effort.",
          "isRequired": false,
          "input": {
            "type": "text",
            "hidden": false
          }
        },
        {
          "name": "description",
          "label": "Description",
          "description": "Provide a brief description of the effort, outlining its purpose, scope, and expected outcomes.",
          "isRequired": false,
          "input": {
            "type": "textarea",
            "hidden": false
          }
        },
        {
          "name": "type",
          "label": "Type",
          "description": "Select the type of effort based on its current intensity: On (most active), Ongoing (steady progress), Simmering (background focus), or Sleeping (on hold).",
          "isRequired": false,
          "input": {
            "type": "select",
            "source": "fixed",
            "options": [
              {
                "value": "On",
                "label": "üî• On"
              },
              {
                "value": "Ongoing",
                "label": "‚ôªÔ∏è Ongoing"
              },
              {
                "value": "Simmering",
                "label": "~ Simmering"
              },
              {
                "value": "Sleeping",
                "label": "üí§ Sleeping"
              }
            ]
          }
        },
        {
          "name": "rank",
          "label": "Rank",
          "description": "Assign a rank to prioritize the effort relative to others.",
          "isRequired": false,
          "input": {
            "type": "slider",
            "min": 0,
            "max": 5
          }
        },
        {
          "name": "eol",
          "label": "End of Life",
          "description": "Indicate if the effort has been cancelled, archived, or ended prematurely.",
          "isRequired": false,
          "condition": {
            "dependencyName": "rank",
            "type": "belowOrEqual",
            "value": 0
          },
          "input": {
            "type": "toggle",
            "hidden": false
          }
        },
        {
          "name": "tags",
          "label": "Tags",
          "description": "Select relevant tags, themes",
          "isRequired": false,
          "input": {
            "type": "tag"
          }
        },
        {
          "name": "aliases",
          "label": "Aliases",
          "description": "Enter relevant aliases for this note.",
          "isRequired": false,
          "input": {
            "type": "text",
            "hidden": false
          }
        }
      ],
      "version": "1",
      "template": {
        "createCommand": true,
        "parsedTemplate": []
      }
    },
